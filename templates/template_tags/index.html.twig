{% block if_test %}
<hr>
    {% if ifVar %}
        hello behram
    {% endif %}
    <br>
    {% if definedCheck is defined %}
        definedCheck: defined
        {% else %}
        definedCheck: not defined
    {% endif %}
    <br>
    {% if emptyCheck is empty %}
        emptyCheck: empty
        {% else %}
        emptyCheck: not empty
    {% endif %}

     <br>
    {% if nullCheck is null %}
        nullCheck: null
        {% else %}
        nullCheck: not null
    {% endif %}

    <br>
    {% if 10 is even %}
        yes even
        {% else %}
        no single
    {% endif %}

    <br>
    {% if iterableCheck is iterable %}
    iterableCheck is iterable
    {% else %}
    iterableCheck is not iterable    
    {% endif %}
{% endblock %}

